DELIMITER //
DROP PROCEDURE IF EXISTS cc_pii_field //
CREATE PROCEDURE cc_pii_field
     (
        IN  p_cid                VARCHAR(255)
      )
BEGIN
START TRANSACTION;

DELETE FROM cache_field
  WHERE
    cid = p_cid;
COMMIT;
END //
