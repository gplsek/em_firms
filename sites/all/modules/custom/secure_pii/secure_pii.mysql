DELIMITER //
DROP PROCEDURE IF EXISTS insert_pii_ssn //
CREATE PROCEDURE insert_pii_ssn
     (
        IN  p_entity                VARCHAR(128)   ,
        IN  p_bundle                VARCHAR(128)   ,
        IN  p_nid                   INT       ,
        IN  p_vid                   INT       ,
        IN  p_lang                  VARCHAR(32)   ,
        IN  p_delta                 INT   ,
        IN  p_ssn                   VARCHAR(255)
      )
BEGIN
START TRANSACTION;

INSERT INTO field_data_field_ssn (
    entity_type,
    bundle,
    entity_id,
    revision_id,
    language,
    delta,
    field_ssn_value
  )
  VALUES (
    p_entity,
    p_bundle,
    p_nid,
    p_vid,
    p_lang,
    p_delta,
    p_ssn
  );
COMMIT;
END //
DELIMITER ;






