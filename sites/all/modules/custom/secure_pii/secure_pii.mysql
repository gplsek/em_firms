DELIMITER //
DROP PROCEDURE IF EXISTS update_pii_ssn //
CREATE PROCEDURE update_pii_ssn(
        IN  p_vid                   INT       ,
        IN  p_lang                  VARCHAR(32)   ,
        IN  p_nid                   INT       ,
        IN  p_delta                 INT       ,
        IN  p_ssn                   VARCHAR(255)
     )
BEGIN
START TRANSACTION;

UPDATE field_data_field_ssn
  SET
    revision_id = p_vid,
    language = p_lang,
    field_ssn_value = p_ssn
  WHERE
    entity_id = p_nid AND
    delta = p_delta;

COMMIT;
END //
DELIMITER ;






