<?xml version='1.0' encoding='UTF-8'?>
<WL5G3N0:definitions name="CarbonServiceDefinitions" targetNamespace="http://integration.carbon.verid.com" xmlns="" xmlns:WL5G3N0="http://schemas.xmlsoap.org/wsdl/" xmlns:WL5G3N1="java:com.verid.carbon.integration.datatypes" xmlns:WL5G3N2="http://integration.carbon.verid.com" xmlns:WL5G3N3="http://schemas.xmlsoap.org/wsdl/soap/">
  <WL5G3N0:types>
    <xs:schema attributeFormDefault="unqualified" elementFormDefault="unqualified" jaxb:version="2.0" targetNamespace="java:com.verid.carbon.integration.datatypes" xmlns="java:com.verid.carbon.integration.datatypes" xmlns:WL5G3N0="http://schemas.xmlsoap.org/wsdl/" xmlns:WL5G3N1="java:com.verid.carbon.integration.datatypes" xmlns:WL5G3N2="http://integration.carbon.verid.com" xmlns:WL5G3N3="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:ns="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://schemas.xmlsoap.org/soap/encoding/" xmlns:s0="http://schemas.xmlsoap.org/wsdl/" xmlns:s1="java:com.verid.carbon.integration.datatypes" xmlns:s2="http://integration.carbon.verid.com" xmlns:s3="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:stns="java:com.verid.carbon.integration.datatypes" xmlns:tns="http://integration.carbon.verid.com" xmlns:types="java:com.verid.carbon.integration.datatypes" xmlns:wsd="http://schemas.xmlsoap.org/wsdl/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
      <xs:simpleType name="account-name-type">
        <xs:restriction base="xs:string"/>
      </xs:simpleType>
      <xs:simpleType name="transaction-direction-type">
        <xs:restriction base="xs:string">
          <xs:enumeration value="buy"/>
          <xs:enumeration value="sell"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="product-category-type">
        <xs:restriction base="xs:string">
          <xs:enumeration value="accounting"/>
          <xs:enumeration value="alcohol"/>
          <xs:enumeration value="amusement-and-recreation-services"/>
          <xs:enumeration value="apparel"/>
          <xs:enumeration value="audio"/>
          <xs:enumeration value="auto-rental-and-Leasing"/>
          <xs:enumeration value="automotive-service"/>
          <xs:enumeration value="baby"/>
          <xs:enumeration value="background-check"/>
          <xs:enumeration value="bond"/>
          <xs:enumeration value="book"/>
          <xs:enumeration value="cable-or-pay-television-services"/>
          <xs:enumeration value="caller-id-spoofing"/>
          <xs:enumeration value="camera"/>
          <xs:enumeration value="certificate-of-deposit"/>
          <xs:enumeration value="computer"/>
          <xs:enumeration value="concert"/>
          <xs:enumeration value="construction-material"/>
          <xs:enumeration value="cosmetic"/>
          <xs:enumeration value="credit-check"/>
          <xs:enumeration value="electronics"/>
          <xs:enumeration value="equipment-rental"/>
          <xs:enumeration value="footwear"/>
          <xs:enumeration value="furniture-or-home-furnishings"/>
          <xs:enumeration value="future"/>
          <xs:enumeration value="game-or-toy"/>
          <xs:enumeration value="gas"/>
          <xs:enumeration value="grocery"/>
          <xs:enumeration value="hotel"/>
          <xs:enumeration value="insurance"/>
          <xs:enumeration value="investment-advice"/>
          <xs:enumeration value="jewelry"/>
          <xs:enumeration value="kitchen-and-housewares"/>
          <xs:enumeration value="lawn-and-garden-equipment"/>
          <xs:enumeration value="membership-organizations"/>
          <xs:enumeration value="money-transfer-in"/>
          <xs:enumeration value="money-transfer-out"/>
          <xs:enumeration value="motor-vehicle"/>
          <xs:enumeration value="music"/>
          <xs:enumeration value="musical-instrument"/>
          <xs:enumeration value="office-product"/>
          <xs:enumeration value="option"/>
          <xs:enumeration value="pet-supplies"/>
          <xs:enumeration value="real-estate"/>
          <xs:enumeration value="restaurant"/>
          <xs:enumeration value="software"/>
          <xs:enumeration value="sporting-and-athletic-product"/>
          <xs:enumeration value="sporting-event"/>
          <xs:enumeration value="sports-and-recreation-club"/>
          <xs:enumeration value="stock"/>
          <xs:enumeration value="subscriptions-on-line-content"/>
          <xs:enumeration value="subscriptions-other"/>
          <xs:enumeration value="subscriptions-periodical"/>
          <xs:enumeration value="tobacco"/>
          <xs:enumeration value="tools-and-hardware"/>
          <xs:enumeration value="trucking-or-courier-service"/>
          <xs:enumeration value="video-purchase"/>
          <xs:enumeration value="video-rental"/>
          <xs:enumeration value="wager"/>
          <xs:enumeration value="other"/>
          <xs:enumeration value="deposit"/>
          <xs:enumeration value="withdrawal"/>
          <xs:enumeration value="bill-payment"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="product-type">
        <xs:sequence>
          <xs:element name="product-category" type="stns:product-category-type"/>
          <xs:element minOccurs="0" name="model" type="xs:string"/>
          <xs:element minOccurs="0" name="vendor" type="xs:string"/>
          <xs:element minOccurs="0" name="code" type="xs:string"/>
          <xs:element minOccurs="0" name="symbol" type="xs:string"/>
          <xs:element minOccurs="0" name="transaction-direction" type="stns:transaction-direction-type"/>
          <xs:element minOccurs="0" name="product-quantity" type="xs:integer"/>
          <xs:element minOccurs="0" name="per-item-amount" type="xs:decimal"/>
          <xs:element minOccurs="0" name="transaction-date" type="stns:transaction-date-type"/>
          <xs:element minOccurs="0" name="data" type="stns:complex-detail-type"/>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="person-type">
        <xs:annotation>
          <xs:documentation>This construct captures information about the person being authenticated</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" name="name-prefix" type="xs:string">
            <xs:annotation>
              <xs:documentation>Person's name's prefix if any (e.g., Mr., Mrs., Dr., ...)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="name-first" type="xs:string">
            <xs:annotation>
              <xs:documentation>Person's given name</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="name-middle" type="xs:string">
            <xs:annotation>
              <xs:documentation>Person's middle name</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="name-last" type="xs:string">
            <xs:annotation>
              <xs:documentation>Person's surname</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="name-suffix" type="xs:string">
            <xs:annotation>
              <xs:documentation>Person's name's suffix (e.g., Jr, III, ...)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="customer-id" type="stns:customer-id-type"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="email" type="stns:email-type"/>
          <xs:element minOccurs="0" name="ssn" type="xs:string">
            <xs:annotation>
              <xs:documentation>Person's social security number (if provided)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="ssn-type" type="stns:ssn-type">
            <xs:annotation>
              <xs:documentation>Type of the social security number provided (none, last4 or full)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="drivers-license-number" type="xs:string">
            <xs:annotation>
              <xs:documentation>Person's drivers license number (if provided)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="drivers-license-state" type="xs:string">
            <xs:annotation>
              <xs:documentation>State where drivers license issued (if provided, must be a valid 2 letter USPS state code)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="birthdate" type="stns:birthdate-type">
            <xs:annotation>
              <xs:documentation>Person's birthdate</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="address" type="stns:address-type">
            <xs:annotation>
              <xs:documentation>Addresses associated with the person</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="uk-address" type="stns:uk-address-type">
            <xs:annotation>
              <xs:documentation>UK Addresses associated with the person</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="phone-number" type="stns:phone-number-type">
            <xs:annotation>
              <xs:documentation>Phone numbers associated with the person</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="business" type="stns:business-type"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="employer" type="stns:business-type"/>
          <xs:element minOccurs="0" name="occupation" type="xs:string"/>
          <xs:element minOccurs="0" name="risk-assessment" type="stns:risk-assessment-type"/>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="address-type">
        <xs:annotation>
          <xs:documentation>Multi-purpose address container</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="address-street1" type="xs:string">
            <xs:annotation>
              <xs:documentation>Part 1 of the street name</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="address-street2" type="xs:string">
            <xs:annotation>
              <xs:documentation>Part 2 of the street name</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="suite" type="xs:string">
            <xs:annotation>
              <xs:documentation>Suite or apartment number</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="address-city" type="xs:string">
            <xs:annotation>
              <xs:documentation>City of the address</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="address-state" type="xs:string">
            <xs:annotation>
              <xs:documentation>State of the address (must be a valid 2 letter USPS state code)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="address-zip" type="xs:string">
            <xs:annotation>
              <xs:documentation>Zip of the address</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="address-zip-plus4" type="xs:string">
            <xs:annotation>
              <xs:documentation>Zip4 of the address</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="address-country" type="xs:string">
            <xs:annotation>
              <xs:documentation>Not required: Country of the address (assumed to be US)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="address-context" type="stns:address-context-type">
            <xs:annotation>
              <xs:documentation>The relationship between the address and the owning construct</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="address-years-at" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>Years of residence at this address (1.3, 0.2, etc.)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="occupation-start-date" type="xs:date"/>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="uk-address-type">
        <xs:annotation>
          <xs:documentation>Multi-purpose address container</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <!--
					<xs:element name="flat-number" type="xs:string">
						<xs:annotation>
							<xs:documentation>Flat number</xs:documentation>
						</xs:annotation>
					</xs:element>
					-->
          <xs:element name="house-name" type="xs:string">
            <xs:annotation>
              <xs:documentation>House name</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="house-number" type="xs:string">
            <xs:annotation>
              <xs:documentation>House number</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="street1" type="xs:string">
            <xs:annotation>
              <xs:documentation>Street1 name</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="street2" type="xs:string">
            <xs:annotation>
              <xs:documentation>Street2 name</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="district" type="xs:string">
            <xs:annotation>
              <xs:documentation>District name</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="town" type="xs:string">
            <xs:annotation>
              <xs:documentation>Town of the address</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="county" type="xs:string">
            <xs:annotation>
              <xs:documentation>County of the address</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="postcode" type="xs:string">
            <xs:annotation>
              <xs:documentation>Postcode of the address</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="context" type="stns:address-context-type">
            <xs:annotation>
              <xs:documentation>The relationship between the address and the owning construct</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="years-at" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>Years of residence at this address (1.3, 0.2, etc.)</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="phone-number-type">
        <xs:annotation>
          <xs:documentation>Representation of a phone number</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="phone-number" type="xs:string">
            <xs:annotation>
              <xs:documentation>The phone number</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="phone-number-context" type="stns:phone-number-context-type">
            <xs:annotation>
              <xs:documentation>The relationship between the phone number and the owning construct</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="business-type">
        <xs:annotation>
          <xs:documentation>Representation of a business</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="company-name" type="xs:string">
            <xs:annotation>
              <xs:documentation>The name of the business</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="fein" type="xs:string">
            <xs:annotation>
              <xs:documentation>The business's FEIN</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="address" type="stns:address-type">
            <xs:annotation>
              <xs:documentation>Array of addresses associated with the business (typically only one is given)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="phone-number" type="stns:phone-number-type">
            <xs:annotation>
              <xs:documentation>Array of phone numbers associated with the business (typically only one is given)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="title" type="xs:string"/>
          <xs:element minOccurs="0" name="start-date" type="xs:date"/>
          <xs:element minOccurs="0" name="end-date" type="xs:date"/>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ordered-by-type">
        <xs:annotation>
          <xs:documentation>Who is placing the "order".</xs:documentation>
        </xs:annotation>
        <xs:choice>
          <xs:element name="same-as" type="stns:same-as-type"/>
          <xs:element name="person" type="stns:person-type"/>
        </xs:choice>
      </xs:complexType>
      <xs:complexType name="bill-to-type">
        <xs:annotation>
          <xs:documentation>Optional: To whom is the "Order" being billed. Populate either the contact or the same-as field but not both.</xs:documentation>
        </xs:annotation>
        <xs:choice>
          <xs:element name="same-as" type="stns:same-as-type"/>
          <xs:element name="person-type" type="stns:person-type"/>
        </xs:choice>
      </xs:complexType>
      <xs:complexType name="ship-to-type">
        <xs:annotation>
          <xs:documentation>Optional: To whom is the "Order" being shipped. Populate either the contact or the same-as field but not both.</xs:documentation>
        </xs:annotation>
        <xs:choice minOccurs="0">
          <xs:element name="same-as" type="stns:same-as-type"/>
          <xs:element name="person-type" type="stns:person-type"/>
        </xs:choice>
      </xs:complexType>
      <xs:simpleType name="same-as-type">
        <xs:annotation>
          <xs:documentation>Available values for the same-as field.  Note that the bill-to value is only valid when used in the context of the ship-to-type.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="ordered-by">
            <xs:annotation>
              <xs:documentation>Use the ordered-by contact.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="bill-to">
            <xs:annotation>
              <xs:documentation>Use the bill-to contact.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="payment-method">
        <xs:restriction base="xs:string">
          <xs:enumeration value="store-credit"/>
          <xs:enumeration value="gift-card"/>
          <xs:enumeration value="money-transfer"/>
          <xs:enumeration value="cash"/>
          <xs:enumeration value="check"/>
          <xs:enumeration value="credit-card"/>
          <xs:enumeration value="debit-card"/>
          <xs:enumeration value="phone"/>
          <xs:enumeration value="bill-general"/>
          <xs:enumeration value="bill-paper"/>
          <xs:enumeration value="margin"/>
          <xs:enumeration value="other"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="purchase-type">
        <xs:annotation>
          <xs:documentation>Details of the purchase transaction type.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" name="order">
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" name="item" type="stns:product-type"/>
                <xs:element name="total-amount" type="xs:decimal"/>
                <xs:element minOccurs="0" name="total-tax" type="xs:decimal"/>
                <xs:element minOccurs="0" name="total-shipping" type="xs:decimal"/>
                <xs:element minOccurs="0" name="merchant" type="xs:string"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="payment-method" type="stns:payment-method"/>
          <xs:element minOccurs="0" name="shipping" type="stns:shipping-type"/>
          <xs:element name="account" type="stns:account-type"/>
          <xs:element name="venue" type="stns:venue-type"/>
          <xs:element name="transaction-date" type="stns:transaction-date-type"/>
          <xs:element minOccurs="0" name="risk-assessment" type="stns:risk-assessment-type"/>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="mode-type">
        <xs:annotation>
          <xs:documentation>Mode of the transaction. Typically set to "live"</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="simulated"/>
          <xs:enumeration value="testing"/>
          <xs:enumeration value="pilot"/>
          <xs:enumeration value="live"/>
          <xs:enumeration value="verid"/>
          <xs:enumeration value="verid_cs"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="shipping-method">
        <xs:restriction base="xs:string">
          <xs:enumeration value="ground"/>
          <xs:enumeration value="priority-overnight"/>
          <xs:enumeration value="next-day-air"/>
          <xs:enumeration value="in-store-pickup"/>
          <xs:enumeration value="second-day-air"/>
          <xs:enumeration value="priority-ground"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="shipping-type">
        <xs:sequence>
          <xs:element name="bill-to" type="stns:bill-to-type"/>
          <xs:element name="ship-to" type="stns:ship-to-type"/>
          <xs:element minOccurs="0" name="shipping-method" type="stns:shipping-method"/>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="settings-type">
        <xs:annotation>
          <xs:documentation>Any overrides to the controls specified in the transaction's ruleset that are to be used for this particular transaction</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="account-name" type="stns:account-name-type">
            <xs:annotation>
              <xs:documentation>The account name to which this transaction will be associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="mode" type="stns:mode-type">
            <xs:annotation>
              <xs:documentation>The mode that the transaction will run under</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="ruleset" type="xs:string">
            <xs:annotation>
              <xs:documentation>Ruleset that will be used to govern the operation of the transaction.  See your account representative for possible values</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="simulator-mode" type="stns:simulator-mode-type">
            <xs:annotation>
              <xs:documentation>If the mode of the transaction is "simulated" then this field governs the response given by the Verid system</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="attachment-type" type="stns:attachment-type"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="special-feature" type="stns:special-feature-type"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="internationalization" type="stns:internationalization-type"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="reference-id">
            <xs:simpleType>
              <xs:restriction base="stns:reference-id-type"/>
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="disposition-settings-type">
        <xs:complexContent>
          <xs:extension base="stns:settings-type">
            <xs:sequence>
              <xs:element name="transaction-id" type="stns:transaction-id-type"/>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="continue-settings-type">
        <xs:complexContent>
          <xs:extension base="stns:settings-type">
            <xs:sequence>
              <xs:element name="transaction-id" type="stns:transaction-id-type"/>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="resume-settings-type">
        <xs:complexContent>
          <xs:extension base="stns:settings-type">
            <xs:sequence>
              <xs:element name="transaction-id" type="stns:transaction-id-type"/>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="identity-verification-settings-type">
        <xs:complexContent>
          <xs:extension base="stns:settings-type">
            <xs:sequence>
              <xs:element name="task" type="stns:task-type"/>
              <xs:element minOccurs="0" name="sequence-id" type="xs:integer"/>
              <xs:element minOccurs="0" name="agent" type="xs:string"/>
              <xs:element minOccurs="0" name="patriot-act-compliance" type="xs:boolean"/>
              <xs:element minOccurs="0" name="parent-transaction-id" type="stns:transaction-id-type"/>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="verification-type">
        <xs:annotation>
